<?PHP
class Leerlaag {
  public $naam;
  public $llLijst;
  public $aantal;

  function __construct($naam) {
    $this->naam = $naam;
    $this->llLijst = [];
  }
  function voegLeerlingToe($lobject) {
    array_push($this->llLijst,$lobject);
  }
  function get_naam() {
    return $this->naam;
  }
 function get_llLijst() {
    return $this->llLijst;
  }  
}
?>