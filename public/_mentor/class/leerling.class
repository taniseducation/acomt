<?PHP
class Leerling {
  public $naam;
  public $klas;

  function __construct($naam,$klas) {
    $this->naam = $naam;
    $this->klas = $klas;
  }
  function get_naam() {
    return $this->naam;
  }
  function get_klas() {
    return $this->klas;
  }
}

